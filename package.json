{
  "name": "@mockexchange/ui",
  "version": "1.0.0",
  "description": "Mockexchange UI",
  "main": "src/sandbox.ts",
  "scripts": {
    "start": "webpack serve --node-env development --env hot -c ./config/webpack/webpack.config.dev.ts",
    "build": "webpack --node-env production -c ./config/webpack/webpack.config.prod.ts",
    "build:dev": "webpack --node-env development -c ./config/webpack/webpack.config.dev.ts",
    "clean": "rimraf dist *.tgz",
    "test": "jest --coverage --verbose",
    "eslint": "eslint . --ext .tsx,.ts,.js",
    "stylelint": "stylelint src/**/*.scss",
    "lint": "npm run eslint && npm run stylelint"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.k8s.local:mockexchange/mockexchange-ui.git"
  },
  "files": [
    "dist"
  ],
  "author": "jdwheels",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/gitlab-config": "^10.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/pretty": "^2.0.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/sanitize-html": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "babel-jest": "^28.1.1",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.23.3",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "mini-css-extract-plugin": "^2.6.1",
    "pretty": "^2.0.0",
    "react-refresh": "^0.14.0",
    "rimraf": "^3.0.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.2",
    "semantic-release": "^19.0.3",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-standard-scss": "^4.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "nanoid": "^4.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "sanitize-html": "^2.7.0"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/gitlab",
      [
        "@semantic-release/exec",
        {
          "publishCmd": "echo \"RELEASE_VERSION=${nextRelease.version}\" >> release.env"
        }
      ]
    ]
  }
}
